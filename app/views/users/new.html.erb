<% provide(:title, 'ユーザー登録') %>
<h1>ユーザー登録</h1>

<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <%= render 'user_info_input_form', {url: signup_path, btn_comment: "登録"} %>

    <%= render 'shared/load_csv', { url: import_users_path,
                                    explain: "(ヘッダーには「name」と「email」を設定してください。)" } %>
  </div>
</div>