<div class="csv_import_form">
  <%= form_with( url: url, multipart: true, local: true ) do |f| %>
    <%= f.label :file, "csvから読み込む(ファイルは下の例のように記入してください。)" %>
    <br><%= image_tag example_image, size: size %>
    <%= f.file_field :file, accept: '.csv', class: 'form-control' %>
    <%= f.submit "読み込み", class: "btn btn-primary" %>
  <% end %>
</div>